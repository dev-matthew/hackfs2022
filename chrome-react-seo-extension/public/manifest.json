{
  "manifest_version": 3,
  "name": "Extension Name",
  "description": "Extension Decription",
  "version": "0.1.0",
  "icons": {
      "16": "./logo192.png",
      "48": "./logo192.png",
      "128": "./logo192.png"
  },
  "action": {
      "default_icon": "./logo192.png",
      "default_popup": "./index.html",
      "default_title": "Extension Name"
  },
  "background": {
      "service_worker": "./static/js/background.js"
  },
  "permissions": [
    "background",
    "storage",
    "unlimitedStorage",
    "tabs",
    "activeTab",
    "notifications"
  ]
}